{
  "name": "portfoliojameskok",
  "version": "1.0.0",
  "description": "Portfolio van James Kok",
    "scripts": {
      "dev": "echo 'Starting dev server and watching CSS changes...' && concurrently \"tailwindcss -i ./src/styling/input.css -o ./src/styling/tailwindOutput.css --watch\" \"live-server ./src\"",
      "build": "tailwindcss -i ./src/styling/input.css -o ./src/styling/tailwindOutput.css --minify"
    },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kokjp1/Portfolio-James-Kok.git"
  },
  "author": "James Kok",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kokjp1/Portfolio-James-Kok/issues"
  },
  "homepage": "https://github.com/kokjp1/Portfolio-James-Kok#readme",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.18",
    "tailwindcss": "^4.1.3"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.3"
  }
}
